<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Maui.DataForms.Sample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Maui.DataForms.Sample.Views">

    <TabBar>
        <Tab Title="Home">
            <ShellContent ContentTemplate="{DataTemplate views:MainPage}" />
        </Tab>
        <Tab Title="Dynamic Demo">
            <ShellContent ContentTemplate="{DataTemplate views:DynamicDemoPage}" />
        </Tab>
        <Tab Title="Fluent Demo">
            <ShellContent ContentTemplate="{DataTemplate views:FluentDemoPage}" />
        </Tab>
    </TabBar>

</Shell>
